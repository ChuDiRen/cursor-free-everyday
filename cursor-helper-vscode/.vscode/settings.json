{
    "files.exclude": {
        "out": false,
        "node_modules": true
    },
    "search.exclude": {
        "out": true,
        "node_modules": true
    },
    "typescript.tsc.autoDetect": "off",
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "eslint.validate": [
        "typescript"
    ]
}
